#include<stdio.h>
#include<stdlib.h>
int main()
{
    int a[10],b[10],c[10];
    int i;
    for(i=0;i<10;i++)
    {
        a[i]=i;
        b[i]=i;
        c[i]=0;
    }
    for(i=0;i<10;i++)
    {
        c[i]=a[i]+b[i];
    }
    for(i=0;i<10;i++)
    {
       // c[i]=a[i]+b[i];
        printf("%d\n",c[i]);
    }

    return 0;
}
